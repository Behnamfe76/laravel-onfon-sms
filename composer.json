{
    "name": "nineafrica/onfon-sms",
    "description": "Add OnFon SMS to your laravel web application",
    "keywords": [
        "nineafrica",
        "laravel",
        "onfon-sms"
    ],
    "homepage": "https://github.com/nineafrica/onfon-sms",
    "authors": [
        {
            "name": "Nineafrica LTD",
            "email": "packages@nineafrica.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.11.2",
        "illuminate/contracts": "^9.3",
        "ext-curl": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.1.0",
        "orchestra/testbench": "^7.1",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.29",
        "vimeo/psalm": "^4.22"
    },
    "autoload": {
        "psr-4": {
            "Nineafrica\\OnfonSms\\": "src",
            "Nineafrica\\OnfonSms\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nineafrica\\OnfonSms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nineafrica\\OnfonSms\\OnfonSmsServiceProvider"
            ],
            "aliases": {
                "OnfonSms": "Nineafrica\\OnfonSms\\OnfonSmsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
